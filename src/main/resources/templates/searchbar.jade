div(class="mdl-grid")
    form(action="/search", id ="mainSearch")
        span(class="mdl-cell mdl-cell--4col")
            div(class="mdl-textfield mdl-js-textfield")
                input(class="mdl-textfield__input", name="Character", type="text", id="characterID")
                label(class="mdl-textfield__label", for="characterID")
                    |Character Name
            div(class="mdl-textfield mdl-js-textfield")
                input(class="mdl-textfield__input", name="World", type="text", id="worldID", autocomplete="off")
                label(class="mdl-textfield__label", for="worldID")
                    |World
    div(class="mdl-cell mdl-cell--1col")
        button(class="mdl-button mdl-js-button mdl-button--raised mdl-button--accent", type="submit", form="mainSearch")
            |Search
block scripts
    script(src="/scripts/autoCompIt.min.js")
    script(src="/scripts/searchbarAutocomplete.min.js")